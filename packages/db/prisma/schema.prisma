generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Person {
    id   String @id @default(uuid())
    name String
    mass Float
}

model Starship {
    id   String @id @default(uuid())
    name String
    crew Int
}

model Game {
    id           String   @id @default(uuid())
    resourceType String
    leftId       String
    rightId      String
    leftValue    Int
    rightValue   Int
    winnerId     String?
    createdAt    DateTime @default(now())
}
